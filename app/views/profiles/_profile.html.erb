<%= form_with model: @profile, local: true do |f| %>
  <%= f.label :full_name %>
  <%= f.text_field :full_name %>
  <%= f.label :social_name %>
  <%= f.text_field :social_name %>
  <%= f.label :avatar %>
  <%= f.file_field :avatar %><br>
  <%= f.label :birth_date %>
  <%= f.date_field :birth_date %>
  <%= f.label :education %>
  <%= f.text_field :education %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :experience %>
  <%= f.text_field :experience %>
  <%= f.submit 'Registrar perfil' %>
<% end %>
